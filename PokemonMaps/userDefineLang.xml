<NotepadPlus>
	<UserLang name="Pokemon" ext="cmap">
		<Settings>
			<Global caseIgnored="yes"/>
			<TreatAsSymbol comment="yes" commentLine="no"/>
			<Prefix words1="no" words2="no" words3="no" words4="no"/>
		</Settings>
		<KeywordLists>
			<Keywords name="Free">F</Keywords>
			<Keywords name="Grass">G</Keywords>
